if [[ "${WM_CFG_HOST_IS_RPI}" == "true" ]]
then
    WM_HOST_REBOOT=${WM_HOST_REBOOT:-"true"}
    WM_HOST_CLOCK_MANAGEMENT=${WM_HOST_CLOCK_MANAGEMENT:-"true"}
    WM_HOST_SYSTEMD_MANAGEMENT=${WM_HOST_SYSTEMD_MANAGEMENT:-"true"}
    WM_HOST_BLACKLIST_IPV6=${WM_HOST_BLACKLIST_IPV6:-"true"}
    WM_HOST_SSH_DAEMON_MANAGEMENT=${WM_HOST_SSH_DAEMON_MANAGEMENT:-"true"}
    WM_HOST_AVAHI_DAEMON_MANAGEMENT=${WM_HOST_AVAHI_DAEMON_MANAGEMENT:-"true"}
    WM_HOST_FILESYSTEM_MANAGEMENT=${WM_HOST_FILESYSTEM_MANAGEMENT:-"true"}
    WM_HOST_HOSTNAME_MANAGEMENT=${WM_HOST_HOSTNAME_MANAGEMENT:-"true"}
    WM_HOST_WIFI_MANAGEMENT=${WM_HOST_WIFI_MANAGEMENT:-"true"}
    WM_HOST_USER_MANAGEMENT=${WM_HOST_USER_MANAGEMENT:-"true"}
    WM_HOST_TTY_MANAGEMENT=${WM_HOST_TTY_MANAGEMENT:-"true"}
    WM_HOST_KEYBOARD_MANAGEMENT=${WM_HOST_KEYBOARD_MANAGEMENT:-"true"}
    WM_HOST_SUPPORT_MANAGEMENT=${WM_HOST_SUPPORT_MANAGEMENT:-"true"}
    WM_HOST_DBUS_MANAGEMENT=${WM_HOST_DBUS_MANAGEMENT:-"true"}
    WM_HOST_DOCKER_PRUNE_ALL=${WM_HOST_DOCKER_PRUNE_ALL:-"true"}
else
    WM_HOST_REBOOT=${WM_HOST_REBOOT:-"false"}
    WM_HOST_CLOCK_MANAGEMENT=${WM_HOST_CLOCK_MANAGEMENT:-"false"}
    WM_HOST_SYSTEMD_MANAGEMENT=${WM_HOST_SYSTEMD_MANAGEMENT:-"false"}
    WM_HOST_BLACKLIST_IPV6=${WM_HOST_BLACKLIST_IPV6:-"false"}
    WM_HOST_SSH_DAEMON_MANAGEMENT=${WM_HOST_SSH_DAEMON_MANAGEMENT:-"false"}
    WM_HOST_AVAHI_DAEMON_MANAGEMENT=${WM_HOST_AVAHI_DAEMON_MANAGEMENT:-"false"}
    WM_HOST_FILESYSTEM_MANAGEMENT=${WM_HOST_FILESYSTEM_MANAGEMENT:-"false"}
    WM_HOST_HOSTNAME_MANAGEMENT=${WM_HOST_HOSTNAME_MANAGEMENT:-"false"}
    WM_HOST_WIFI_MANAGEMENT=${WM_HOST_WIFI_MANAGEMENT:-"false"}
    WM_HOST_USER_MANAGEMENT=${WM_HOST_USER_MANAGEMENT:-"false"}
    WM_HOST_TTY_MANAGEMENT=${WM_HOST_TTY_MANAGEMENT:-"true"}
    WM_HOST_KEYBOARD_MANAGEMENT=${WM_HOST_KEYBOARD_MANAGEMENT:-"false"}
    WM_HOST_SUPPORT_MANAGEMENT=${WM_HOST_SUPPORT_MANAGEMENT:-"false"}
    WM_HOST_DBUS_MANAGEMENT=${WM_HOST_DBUS_MANAGEMENT:-"true"}
    WM_HOST_DOCKER_PRUNE_ALL=${WM_HOST_DOCKER_PRUNE_ALL:-"false"}
fi